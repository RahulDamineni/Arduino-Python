	# Update force value 
	analogRead(1)
	# Inverse force value and store it in 'th'
	th = 1023 - force
	print th
	if (th < th1)and(flag == 0):
		servo(5,int(p0))

	elif (th > th1)and(th < th2)and(flag == 0):
		k = (th2 - th1)/(p1 - p0)
		p = th/k
		servo(5,int(p))

	elif (th>th2)and(flag == 0):
		servo(5,int(p2))
		flag = 1

	elif (flag == 1):
		servo(5,int(p2))



// Servo test
	servo(5,int(p2))
	delay(1000)
	servo(5,int(p1))
	delay(1000)
	servo(5,int(p0))
	delay(1000)


// Averaging 
	while (counter < 11):
		th1 = 1023 - force
		th2 = th2 + th1
		th = th2/10.0
		counter = counter +1
	
